{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb-base"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": ["off"],
        "linebreak-style": 0,
        "prefer-destructuring": 0,
        "import/extensions": 0,
        "no-param-reassign": 0,
        "import/prefer-default-export": 0,
        "no-shadow": "off"
    }
}
